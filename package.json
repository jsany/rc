{
  "name": "@jsany/rc",
  "version": "1.0.1",
  "description": "personal react components hook",
  "keywords": [
    "react",
    "components",
    "hook"
  ],
  "homepage": "https://github.com/jsany/rc#readme",
  "bugs": {
    "url": "https://github.com/jsany/rc/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jsany/rc.git"
  },
  "license": "MIT",
  "author": "jiangzhiguo2010",
  "sideEffects": [
    ".css",
    ".less",
    "es/**/style/*",
    "lib/**/style/*",
    "src/**/style/**"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "es",
    "lib"
  ],
  "scripts": {
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "release": "standard-version",
    "start": "dumi dev",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "lint-staged": {
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ],
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "classnames": "^2.2.6",
    "react": "^16.12.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@types/classnames": "^2.2.10",
    "@types/react": "^16.9.51",
    "@umijs/test": "^3.0.5",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "devmoji": "^2.1.9",
    "dumi": "^1.0.10",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "jsdom-worker": "^0.2.1",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "sort-package-json": "^1.46.0",
    "standard-version": "^9.0.0",
    "vfile-message": "^2.0.4",
    "yorkie": "^2.0.0"
  },
  "peerDependencies": {
    "node-fetch": "^2.6.1"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "prepare-commit-msg": "devmoji -e --lint"
  },
  "issues": {
    "url": "https://github.com/jsany/rc/issues"
  }
}
